{
  "name": "dev-notes",
  "displayName": "dev.notes",
  "description": "ðŸ’¡ Take quick, powerful dev notesâ€”right from your VS Code sidebar!",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zefty/dev-notes"
  },
  "version": "0.0.1",
  "publisher": "Zefty",
  "engines": {
    "vscode": "^1.99.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "devNotesContainer",
          "title": "dev.notes",
          "icon": "resources/notes.svg"
        }
      ]
    },
    "views": {
      "devNotesContainer": [
        {
          "id": "devNotesNotesTreeView",
          "name": "Your Notes!",
          "icon": "resources/notes.svg"
        },
        {
          "id": "devNotesSelectedNoteWebview",
          "name": "Take your note here!",
          "type": "webview",
          "icon": "resources/notes.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "dev-notes.newNote",
        "title": "New Note",
        "icon": "$(add)"
      },
      {
        "command": "dev-notes.setSelectedNote",
        "title": "Open Note"
      },
      {
        "command": "dev-notes.clearAllNotes",
        "title": "Clear All Notes",
        "icon": "$(trash)"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "dev-notes.newNote",
          "when": "view == devNotesNotesTreeView",
          "group": "navigation"
        },
        {
          "command": "dev-notes.clearAllNotes",
          "when": "view == devNotesNotesTreeView",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.99.0",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.23.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.2",
    "webpack": "^5.98.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "uuid": "^11.1.0"
  }
}